    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">Datahanks</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon justify-content-end"></span>
      </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0 ">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="/">Home</a>
                    </li>

                    <% if(!currentUser){ %>
                        <li class="nav-item"><a class="nav-link " href="/login ">Login</a></li>
                        <li class="active nav-item">
                            <a class="nav-link" href="/register ">Signup</a>
                        </li>
                        <% } else { %>
                            <li class="nav-item">
                                <a class="nav-link" href="/users/<%= currentUser._id %>  ">Currently Logged in as <%=currentUser.username %> </a>
                            </li>
                            <li class="nav-item"><a class="nav-link" href="/logout ">Logout</a></li>
                            <% } %>



                </ul>

            </div>
        </div>
    </nav>











    <div class="container">
        <% if (currentUser && currentUser.isAdmin) { %>
            <H4>You're an admin!!</H4>

            <% } %>
                <!-- <header style=" box-shadow: 10px 10px 50px rgba(0, 0, 0, 0.3); background: linear-gradient(#ffffff, #7b7c7c);" class="jumbotron  ">
                    <div class="container">

                    </div>
                    <h1> <span class="glyphicon glyphicon-hdd" aria-hidden="true"></span>Welcome To The Data hanks</h1>

                    <p>View our handpicked Study Materials from all over the Work.....!!!</p>
                    <a class="btn btn-primary btn-large" href="/hanks/new">Add New hanks</a>


                </header> -->
                <div class="row">
                    <div class="col-md-8">
                        <div class="d-flex align-items-stretch">
                            <div class="row text-center " style="display: flex;flex-wrap: wrap; ">
                                <div class="col-md-8 list-group ">
                                    <div style="margin-left: 1%;" class="card well  col-md-12">
                                        <p class="pull-left">You can add your projects,blogs,etc...</p>
                                        <a class="btn btn-primary btn-md pull pull-right" href="/hanks/new">add something</a>

                                    </div>

                                </div>
                                <div class="col-md-4">
                                    <form action="/hanks" method="GET" class="form-inline">
                                        <div style="padding-top: 20px;" class="form-group">
                                            <input type="text" name="search" placeholder="Search here..." class="form-control">
                                            <input type="submit" value="search" class="btn btn-md  btn-primary">
                                            <hr>
                                        </div>

                                    </form>
                                </div>

                            </div>










                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <p>
                        <button class="btn btn-primary" type="button" data-bs-toggle="collapse" data-bs-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
    Read the latest science news here
  </button>
                    </p>
                    <div class="collapse" id="collapseExample">
                        <div class="">




                        </div>
                    </div>
                </div>


                <div class="col-md-8 list-group centre ">
                    <div class="col-md-12">
                        <h4 class="list-group-item active">News Feed</h4>
                        <hr>
                        <% hanks.forEach(function(hank){ %>
                            <div class="card" style="box-shadow: rgba(0, 0, 0, 0.15) 0px 5px 15px 0px;">
                                <div class="pull-left" style="height:auto;max-width: 100%; background-color: white;">
                                    <div class="pull-left " style="display: flex;justify-content: space-between;">
                                        <h5> <img src="data:image/png;base64,"
                                                height="50" width="50" alt="">
                                            <h4 style="padding-left: 5px;">
                                                <%= "   "+ hank.author.username  +"  added new post"%>
                                                    <div class="" style="word-wrap: break-word;">
                                                        <p style="font-size: 1rem;font-family: Arial, Helvetica, sans-serif; padding-top: 10px;">
                                                            <%-"on " + hank.timeCreated.toString().substring(3,16) %>
                                                        </p>

                                                    </div>
                                            </h4>

                                        </h5>


                                    </div>
                                    <div class="pull-left container" style="max-width: 100%;">
                                        <p style="word-wrap: break-word;text-overflow: ellipsis;">
                                            <%= hank.snippet + "..."  %>
                                        </p>

                                    </div>





                                    <div class="" style="max-width: 100%;">
                                        <img class="img img-fluid postt thumbnail" style="max-width: 100%;" src="<%= hank.image %>" alt="">
                                        <hr>
                                    </div>

                                    <!-- <div>
                                        <span style="background-color: #337AB7;" class="badge label-primary"><i class="fas fa-thumbs-up"></i> <%= hank.likes.length %></span>
                                    </div> -->
                                    <div class="likesection">


                                        <form action="/hanks/<%= hank._id %>/like" method="post">
                                            <div class="btn-group pull-left">
                                                <% if (currentUser && hank.likes.some(function (like) {
                                                return like.equals(currentUser._id)
                                            })) { %>
                                                    <button class="btn btn-sm btn-primary">
                                                    <i class="fas fa-thumbs-up"></i>  <%= hank.likes.length %>
                                                </button>
                                                    <% } else { %>
                                                        <button class="btn btn-sm btn-secondary">
                                                    <i class="fas fa-thumbs-up"></i> Like (<%= hank.likes.length %>)
                                                </button>
                                                        <% } %>

                                            </div>
                                        </form>
                                    </div>


                                    <form action="/hanks/<%= hank._id  %>/comments " method="post" style="max-width: 100%;display: block;">
                                        <div class="btn-group " style="max-width: 100%;">

                                            <textarea class="form-control pull-left" name="comment[text]" cols="75" rows="2" placeholder="leave a comment" style="height: 59px; margin: 0px; width: 564px;"></textarea>
                                            <div class="form-group pull-right">
                                                <button class="btn btn-md btn-primary btn-block">comment</button>
                                            </div>


                                        </div>
                                    </form>
                                    <div class="btn btn-primry pull-right">
                                        <button class="btn btn-md btn-primary">
                                            <a style="color: white;" href="/hanks/<%= hank._id%>">View Post</a>
                                        </button>
                                    </div>



                                </div>
                            </div>
                            <hr style="max-width: 100%; font-size: larger; height: 5px;text-shadow: 5px 5px 5px 5px;">

                            <% })%>
                    </div>
                </div>
    </div>

    </div>

    </div>










    <hr>
    </div>

    </div>



    </div>
    </div>


</body>

