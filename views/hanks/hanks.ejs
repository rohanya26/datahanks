<body>
    <style>
        body {
            overflow-x: hidden;
            word-wrap: break-word;
        }
        
        @media only screen and (max-width: 600px) {
            body {
                background-color: white;
            }
            .postt {
                height: 200px;
                width: 420px;
            }
            .likesection {
                margin-top: 3%;
                max-width: 90%;
            }
            .form-control {
                margin-top: 2%;
                max-width: 100%;
                border: none;
                border-bottom: 2px solid black;
            }
            .centre {
                margin: auto;
                width: 100%;
                margin-left: 5%;
            }
            .textarea {
                border: none;
            }
            .placeholder {
                margin-bottom: 1%;
            }
        }
    </style>
